{
    "manifest_version": 3,
    "name": "dbec",
    "version": "1.0",
  
    "description": "Detect Business Email Compromise. Returns data on age of email domain registration.",
  
    "icons": {
      "48": "icons/mail-lock.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.google.com/mail/*"],
        "js": ["dbec.js"]
      }
    ],

    "host_permissions": [
      "*://*.google.com/mail/*"
    ]
  }
  